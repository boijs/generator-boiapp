<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <title><%= appname %></title>
        <% for (var i=0,len=styleLibs.length;i<len;i++) { %>
            <link rel="stylesheet" href="<%= styleLibs[i] %>"></link>
        <% } %>
    </head>
    <body>
        <div class="nav">
          <ul class='nav__list'>
            <li class='nav__item'>
              <% if(enableSprites){ %>
                <i class="icon icon__home"></i>
              <% } %>
              <span class="item__label">首页</span>
            </li>
            <li class='nav__item'>
              <a href="./about.html">
              <% if(enableSprites){ %>
                <i class="icon icon__about"></i>
              <% } %>
              <span class="item__label">关于</span>
              </a>
            </li>
          </ul>
        </div>
        <div id="app"></div>
        <% for (var i=0,len=jsLibs.length;i<len;i++) { %>
        <script src="<%= jsLibs[i] %>" charset="utf-8"></script>
        <% } %>
    </body>
</html>
